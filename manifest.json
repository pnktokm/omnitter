{
  "manifest_version": 2,
  "name": "omnitter",
  "description": "qq",
  "version": "1.0",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "omnibox": { "keyword": "qq" },
  "background": {
    "scripts": [
      "js/lib/codebird.js",
      "js/background.js"
    ]
  },
  "web_accessible_resources": ["templates/oauth_callback.html"],
  "permissions": [
    "tabs",
    "storage",
    "http://*/*",
    "https://*/*"
  ]
}
